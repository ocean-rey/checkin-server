generator client {
  provider = "prisma-client-js"
}

datasource db{
    provider = "postgresql"
    url = "postgres://postgres@localhost:5432"
}

model User{
  id String @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  email String @unique
  check_ins CheckIn[]
}

model Event{
  id String @id @default(uuid())
  name String
  startDate DateTime
  endDate DateTime
  lat Float
  long Float
  city String
  image String
  shortDesc String
  check_ins CheckIn[]
}

model CheckIn{
  id String @id @default(uuid())
  user_fk String
  event_fk String
  user User @relation(fields: [user_fk], references: id)
  event Event @relation(fields: [event_fk], references: id)
}

